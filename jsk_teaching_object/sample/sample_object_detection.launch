<launch>

  <param name="/use_sim_time" value="true"/>

  <node name="rosbag_play" pkg="rosbag" type="play"
        args="$(find jsk_teaching_object)/sample/data/2023-11-16-drink-object.bag --clock --loop"/>

  <group ns="usb_cam">
    <node name="republish"
          pkg="image_transport" type="republish"
          args="compressed raw">
      <remap from="in" to="image_raw"/>
      <remap from="out" to="image_raw"/>
    </node>
  </group>

  <include file="$(find jsk_teaching_object)/launch/object_detection.launch" >
    <arg name="launch_usb_cam" value="false" />
  </include>

</launch>
